<?xml version="1.0" encoding="utf-8"?>
<project>
	
	<meta title="moonshine-feathersui-text-editor" package="Moonshine" version="1.0.0" company="Prominic.NET" />
	<app main="openfl.display.Sprite" file="moonshine-feathersui-text-editor" />

	<window allow-high-dpi="true"/>
	<window fps="60"/>
	
	<source path="src" />
	
	<haxelib name="openfl" />
	<haxelib name="actuate" />
	<haxelib name="feathersui" />
	<haxelib name="markdown-openfl-textfield" />
	<haxelib name="moonshine-openfl-language-client" />

	<template path="templates" if="flash"/>

	<haxeflag name="--macro" value="include('moonshine.editor')"/>
	<haxeflag name="-dce" value="no" if="flash"/>
</project>